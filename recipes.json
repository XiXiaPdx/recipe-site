      "dairy": false },
    {"ingredientName": "butter",
      "quantity": 1,
      "unit": "tablespoon",
      "meat": false,
      "dairy": true },
    {"ingredientName": "olive oil",
      "quantity": 1,
      "unit": "tablespoon",
      "meat": false,
      "dairy": false },
    {"ingredientName": "chopped parsley",
      "quantity": 1,
