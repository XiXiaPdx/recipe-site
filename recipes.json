pecanCrustedTrout = {"recipeName": "pecan-crusted-trout",
  "displayName": "Pecan-Crusted Trout",
  "ingredients": [
    {"ingredientName": "flour",
      "quantity": 2,
      "unit": "tablespoon",
      "meat": false,
      "dairy": false },
    {"ingredientName": "nonfat buttermilk",
      "quantity": 0.25,
      "unit": "cup",
      "meat": false,
      "dairy": true },
    {"ingredientName": "pecan halves, ground",
